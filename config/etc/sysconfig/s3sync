LOG_FILE="/var/log/s3sync.log"

# Where file backups store
FILE_BACKUP_DIR="/var/backups/files"

# Where mysql backups store
MYSQL_BACKUP_DIR="/var/backups/mysql"

# Directories to back up
MIRROR_DIR="/var/backups/mirror"

